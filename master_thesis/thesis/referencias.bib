isbn = {2167-4817},
issn = {2167-4817},
journal = {Current Surgery Reports},
keywords = {pattern recognition {\'{a}},prosthetic {\'{a}} myoelectric {\'{a}},regression control {\'{a}} virtual,rehabilitation environment},
number = {3},
