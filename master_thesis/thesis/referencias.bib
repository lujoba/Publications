journal = {The Journal of Rehabilitation Research and Development},
keywords = {amputee, control, EMG, feedback, forearm, hand, my},
number = {6},
pages = {719},
pmid = {21938658},
title = {{Myoelectric forearm prostheses: State of the art from a user-centered perspective}},
