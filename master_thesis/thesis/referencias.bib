journal = {Proceedings of the Annual International Conference of the IEEE Engineering in Medicine and Biology Society, EMBS},
keywords = {Motor neuroprostheses - Prostheses,Neuromuscular s},
pages = {1140--1143},
pmid = {26736467},
title = {{Offline accuracy: A potentially misleading metric in myoelectric pattern recognition for prosthetic control}},
volume = {2015-Novem},
year = {2015}
}
